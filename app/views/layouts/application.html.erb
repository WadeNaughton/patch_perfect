<!DOCTYPE html>
<html>
  <head>
    <title>PatchPerfect</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= render "layouts/header" %>
  </head>

  <body>
  <div class="container">



        <% flash.each do |type, msg| %>
        <div>
        <%= msg %>
        </div>
        <% end %>
        <%= yield %>
  </div>

  </body>
  <%= render "layouts/footer" %>
</html>
