<%= form_with url: "/register", method: :post, local:true do |form| %>

  <%= form.label :first_name %>
  <p><%= form.text_field :first_name %></p>

  <%= form.label :last_name %>
  <p><%= form.text_field :last_name %></p>

  <%= form.label :username %>
  <p><%= form.text_field :username %></p>

  <%= form.label :email %>
  <p><%= form.text_field :email %></p>

  <%= form.label :password %>
  <p><%= form.text_field :password %></p>

  <%= form.label :password_confirmation %>
  <p><%= form.text_field :password_confirmation %></p>

  <%= form.submit "Submit" %>
<% end %>
